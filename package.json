{
  "name": "watashino",
  "version": "1.12.0",
  "private": true,
  "description": "My personal website about my journey as a developer, powered by Gatsby.",
  "keywords": [
    "gatsby",
    "react",
    "styled-components",
    "typescript"
  ],
  "homepage": "https://jeremierodriguez.com",
  "bugs": {
    "url": "https://gitlab.com/jeremiergz/watashino/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/jeremiergz/watashino.git"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "files": [
    "public/"
  ],
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf .cache coverage dist out public",
    "compile": "gatsby build",
    "compile:typings": "graphql-codegen",
    "lint": "eslint --ext ts,tsx src",
    "lint:tsc": "tsc --noEmit",
    "prerelease": "run-s build lint lint:tsc test",
    "release": "node release.js",
    "start": "gatsby serve",
    "start:w": "gatsby develop",
    "test": "jest",
    "test:cc": "jest --clearCache",
    "version": "node --print process.env.npm_package_version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*/*.{ts,tsx}",
      "!**/*/*.d.ts"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "verbose": true
  },
  "dependencies": {
    "@react-google-maps/api": "1.8.3",
    "copy-to-clipboard": "3.3.1",
    "gatsby": "2.19.22",
    "gatsby-image": "2.2.41",
    "gatsby-plugin-google-analytics": "2.1.36",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "2.2.42",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-react-helmet-async": "1.0.15",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-sitemap": "2.2.27",
    "gatsby-plugin-styled-components": "3.1.19",
    "gatsby-plugin-typescript": "2.1.27",
    "gatsby-remark-images": "3.1.44",
    "gatsby-remark-vscode": "2.0.2",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-transformer-json": "2.2.26",
    "gatsby-transformer-remark": "2.6.53",
    "gatsby-transformer-sharp": "2.3.14",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet-async": "1.0.4",
    "rehype-react": "4.0.1",
    "styled-components": "5.0.1",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@graphql-codegen/add": "^1.12.2",
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@kobionic/eslint-config-typescript-react": "^1.0.1",
    "@types/googlemaps": "^3.39.3",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-test-renderer": "^16.9.2",
    "@types/rehype-react": "^4.0.0",
    "@types/styled-components": "^5.0.1",
    "@types/styled-system": "^5.1.7",
    "babel-jest": "^25.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.2.29",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "graphql": "^14.6.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "mdast-util-to-hast": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.12.0",
    "remark-parse": "^7.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2",
    "unified": "^8.4.2"
  },
  "engines": {
    "node": ">=12"
  }
}
