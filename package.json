{
  "name": "watashino",
  "version": "0.1.0",
  "private": true,
  "description": "My personal website about my journey as a developer, powered by Gatsby.",
  "keywords": [
    "gatsby",
    "react",
    "styled-components",
    "typescript"
  ],
  "homepage": "https://jeremierodriguez.com",
  "bugs": {
    "url": "https://gitlab.com/jeremiergz/watashino/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/jeremiergz/watashino.git"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "files": [
    "public/**/*"
  ],
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf .cache coverage dist out public",
    "compile": "gatsby build",
    "compile:typings": "graphql-codegen",
    "lint": "eslint --ext ts,tsx src",
    "start": "gatsby serve",
    "start:w": "gatsby develop",
    "test": "jest",
    "test:cc": "jest --clearCache",
    "version": "node --print process.env.npm_package_version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*/*.{ts,tsx}",
      "!**/*/*.d.ts"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "verbose": true
  },
  "dependencies": {
    "@react-google-maps/api": "1.6.0",
    "gatsby": "2.15.6",
    "gatsby-image": "2.2.17",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "2.2.13",
    "gatsby-plugin-react-helmet": "3.1.6",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "2.2.19",
    "gatsby-plugin-styled-components": "3.1.4",
    "gatsby-plugin-typescript": "2.1.6",
    "gatsby-source-filesystem": "2.1.20",
    "gatsby-transformer-json": "2.2.7",
    "gatsby-transformer-remark": "2.6.21",
    "gatsby-transformer-sharp": "2.2.13",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "styled-components": "4.3.2",
    "styled-system": "5.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@graphql-codegen/add": "^1.7.0",
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/typescript": "^1.7.0",
    "@graphql-codegen/typescript-operations": "^1.7.0",
    "@kobionic/eslint-config-typescript": "^1.0.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "@types/react-test-renderer": "^16.9.0",
    "@types/styled-components": "^4.1.18",
    "@types/styled-system": "^5.1.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.12",
    "eslint-plugin-react": "^7.14.3",
    "graphql": "^14.5.4",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": ">=12"
  }
}
