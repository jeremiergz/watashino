{
  "name": "watashino",
  "version": "22.04.1",
  "private": true,
  "description": "My personal website about my journey as a developer, powered by Gatsby.",
  "keywords": [
    "gatsby",
    "react",
    "tailwindcss",
    "typescript"
  ],
  "type": "commonjs",
  "homepage": "https://jeremierodriguez.com",
  "bugs": {
    "url": "https://github.com/jeremiergz/watashino/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremiergz/watashino.git"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "files": [
    "public/"
  ],
  "scripts": {
    "build": "run-s clean build:compile",
    "build:compile": "gatsby build",
    "clean": "rimraf .cache/ coverage/ package/ public/ watashino-*.tgz *.tsbuildinfo",
    "lint": "run-p lint:eslint lint:tsc",
    "lint:eslint": "eslint --ext ts,tsx src",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky install",
    "prerelease": "run-s build lint test",
    "release": "make release",
    "start": "gatsby serve",
    "start:debug": "NODE_OPTIONS=--inspect gatsby develop",
    "start:w": "gatsby develop",
    "test": "jest",
    "test:cc": "jest --clearCache",
    "version": "node --print process.env.npm_package_version"
  },
  "dependencies": {
    "autoprefixer": "10.4.4",
    "clsx": "1.1.1",
    "date-fns": "2.28.0",
    "gatsby": "4.11.2",
    "gatsby-plugin-image": "2.11.1",
    "gatsby-plugin-manifest": "4.11.1",
    "gatsby-plugin-offline": "5.11.1",
    "gatsby-plugin-postcss": "5.11.0",
    "gatsby-plugin-react-helmet": "5.11.0",
    "gatsby-plugin-robots-txt": "1.7.0",
    "gatsby-plugin-root-import": "2.0.8",
    "gatsby-plugin-sharp": "4.11.1",
    "gatsby-plugin-sitemap": "5.11.1",
    "gatsby-source-filesystem": "4.11.1",
    "gatsby-transformer-sharp": "4.11.0",
    "gatsby-transformer-yaml": "4.11.0",
    "postcss": "8.4.12",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-helmet": "6.1.0",
    "tailwindcss": "3.0.23",
    "typeface-open-sans": "1.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@kobionic/eslint-config-typescript": "^4.0.3",
    "@kobionic/eslint-config-typescript-react": "^2.0.3",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-helmet": "^6.1.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/tailwindcss": "^3.0.10",
    "babel-jest": "^27.5.1",
    "babel-preset-gatsby": "^2.11.1",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "react-test-renderer": "18.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  }
}
