{
  "name": "watashino",
  "version": "20.08.6",
  "private": true,
  "description": "My personal website about my journey as a developer, powered by Gatsby.",
  "keywords": [
    "gatsby",
    "react",
    "styled-components",
    "typescript"
  ],
  "homepage": "https://jeremierodriguez.com",
  "bugs": {
    "url": "https://github.com/jeremiergz/watashino/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremiergz/watashino.git"
  },
  "license": "Apache-2.0",
  "author": "Jeremie Rodriguez <contact@jeremierodriguez.com> (https://github.com/jeremiergz)",
  "files": [
    "public/"
  ],
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf .cache coverage dist out public",
    "compile": "gatsby build",
    "deploy": "gh-pages --dist public --repo https://${GH_TOKEN}@github.com/jeremiergz/watashino.git",
    "lint": "run-p lint:eslint lint:tsc",
    "lint:eslint": "eslint --ext ts,tsx src",
    "lint:tsc": "tsc --noEmit",
    "prerelease": "npm-run-all --parallel build lint --sequential test",
    "release": "node release.js",
    "start": "gatsby serve",
    "start:w": "gatsby develop",
    "test": "jest",
    "test:cc": "jest --clearCache",
    "version": "node --print process.env.npm_package_version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*/*.{ts,tsx}",
      "!**/*/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".cache"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "modulePaths": [
      "src"
    ],
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "verbose": true
  },
  "dependencies": {
    "copy-to-clipboard": "3.3.1",
    "gatsby": "2.24.47",
    "gatsby-image": "2.4.16",
    "gatsby-plugin-google-analytics": "2.3.13",
    "gatsby-plugin-google-fonts": "1.0.1",
    "gatsby-plugin-manifest": "2.4.23",
    "gatsby-plugin-offline": "3.2.23",
    "gatsby-plugin-react-helmet-async": "1.0.16",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sharp": "2.6.27",
    "gatsby-plugin-sitemap": "2.4.11",
    "gatsby-plugin-styled-components": "3.3.10",
    "gatsby-remark-images": "3.3.25",
    "gatsby-remark-vscode": "3.0.1",
    "gatsby-source-filesystem": "2.3.24",
    "gatsby-transformer-json": "2.4.11",
    "gatsby-transformer-remark": "2.8.28",
    "gatsby-transformer-sharp": "2.5.13",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet-async": "1.0.6",
    "rehype-react": "6.1.0",
    "styled-components": "5.1.1",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@kobionic/eslint-config-typescript": "^3.0.4",
    "@kobionic/eslint-config-typescript-react": "^1.0.5",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^16.9.3",
    "@types/rehype-react": "^4.0.0",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.10",
    "babel-jest": "^26.3.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-gatsby": "^0.5.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "jest-styled-components": "^7.0.3",
    "mdast-util-to-hast": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "16.13.1",
    "remark-parse": "^8.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7",
    "unified": "^9.2.0"
  },
  "engines": {
    "node": ">=12"
  }
}
